{
	"name": "@just-web/i18n",
	"version": "1.0.6",
	"description": "just-web i18n plugin module",
	"keywords": [
		"just-web",
		"framework"
	],
	"homepage": "https://github.com/justland/just-web/tree/main/plugins/i18n",
	"bugs": {
		"url": "https://github.com/justland/just-web/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/justland/just-web.git",
		"directory": "plugins/i18n"
	},
	"license": "MIT",
	"author": {
		"name": "Homa Wong (unional)",
		"email": "homawong@gmail.com",
		"url": "https://github.com/unional"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./esm/index.d.ts",
				"default": "./esm/index.js"
			},
			"require": {
				"types": "./cjs/index.d.cts",
				"default": "./cjs/index.cjs"
			},
			"default": "./esm/index.js"
		},
		"./testing": {
			"import": {
				"types": "./esm/testing/index.d.ts",
				"default": "./esm/testing/index.js"
			},
			"require": {
				"types": "./cjs/testing/index.d.ts",
				"default": "./cjs/testing/index.js"
			},
			"default": "./esm/testing/index.js"
		}
	},
	"main": "./cjs/index.cjs",
	"module": "./esm/index.js",
	"types": "./cjs/index.d.cts",
	"files": [
		"cjs",
		"esm",
		"testing",
		"ts",
		"!**/*.{spec,test,unit,accept,integrate,system,perf,stress}.*"
	],
	"scripts": {
		"build": "tsdown",
		"clean": "rimraf cjs esm coverage tslib --glob *.tsbuildinfo",
		"coverage": "vitest run --coverage",
		"nuke": "rimraf node_modules",
		"size": "size-limit",
		"test": "vitest run",
		"test:watch": "vitest",
		"w": "pnpm test:watch",
		"verify": "npm-run-all -p build lint coverage -p size"
	},
	"devDependencies": {
		"@just-web/app": "workspace:^",
		"@just-web/commands": "workspace:^",
		"@just-web/keyboard": "workspace:^",
		"@just-web/repo-scripts": "workspace:^",
		"@repobuddy/typescript": "^1.2.0",
		"@repobuddy/vitest": "^2.1.0",
		"@size-limit/preset-small-lib": "~11.2.0",
		"@vitest/coverage-v8": "^4.0.15",
		"cross-env": "^7.0.3",
		"npm-run-all2": "^6.0.0",
		"repobuddy": "^1.0.1",
		"rimraf": "~5.0.0",
		"size-limit": "~11.2.0",
		"tsdown": "^0.15.12",
		"vitest": "^4.0.15"
	},
	"peerDependencies": {
		"@just-web/app": "workspace:^",
		"@just-web/commands": "workspace:^",
		"@just-web/keyboard": "workspace:^"
	},
	"peerDependenciesMeta": {
		"@just-web/commands": {
			"optional": true
		},
		"@just-web/keyboard": {
			"optional": true
		}
	}
}

{
  "name": "@just-web/keyboard",
  "version": "1.1.1",
  "description": "just-web keyboard plugin module",
  "keywords": [
    "just-web",
    "framework"
  ],
  "homepage": "https://github.com/justland/just-web/tree/main/plugins/keyboard",
  "bugs": {
    "url": "https://github.com/justland/just-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justland/just-web.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com",
    "url": "https://github.com/unional"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!**/*.spec.*"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "rimraf coverage lib *.tsbuildinfo",
    "coverage": "jest --coverage 2>&1",
    "depcheck": "depcheck",
    "lint": "eslint --ext=ts,tsx,js,yaml,yml .",
    "nuke": "run-s clean nuke:local",
    "nuke:local": "rimraf node_modules || true",
    "test": "jest 2>&1",
    "test:watch": "jest --watch",
    "verify": "run-p build lint coverage depcheck",
    "verify:build": "run-s clean build depcheck"
  },
  "dependencies": {
    "type-plus": "^4.13.1"
  },
  "devDependencies": {
    "@babel/core": "~7.19.0",
    "@babel/plugin-transform-runtime": "~7.19.0",
    "@just-web/log": "workspace:^*",
    "@just-web/os": "workspace:^*",
    "@just-web/repo-scripts": "workspace:^*",
    "@just-web/states": "workspace:^*",
    "@just-web/testing": "workspace:^*",
    "@just-web/types": "workspace:^*",
    "@size-limit/preset-small-lib": "~7.0.8",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "depcheck": "~1.4.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-harmony": "^7.0.0",
    "eslint-plugin-yml": "~0.15.0",
    "jest": "^27.5.1",
    "jest-validate": "^27.5.1",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^1.0.0",
    "npm-check-updates": "~12.5.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "~3.0.2",
    "size-limit": "~7.0.8",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@just-web/log": "workspace:^*",
    "@just-web/os": "workspace:^*",
    "@just-web/states": "workspace:^*",
    "@just-web/types": "workspace:^*"
  },
  "size-limit": [
    {
      "limit": "15 kb"
    }
  ],
  "typedoc": {
    "entryPoint": "./ts/index.ts",
    "readmeFile": "./README.md"
  }
}

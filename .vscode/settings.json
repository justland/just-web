{
  "cSpell.words": [
    "codeql",
    "noreply",
    "npmjs",
    "pnpm",
    "pnpmfile",
    "vite"
  ],
  "files.associations": {
    "**/common/config/rush/*.json": "jsonc",
    "common/git-hooks/*": "shell",
    "rush.json": "jsonc"
  },
  "json.schemas": [
    {
      "fileMatch": [
        "tsconfig.*.json"
      ],
      "url": "http://json.schemastore.org/tsconfig"
    }
  ]
}

{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ES2020",
    "moduleResolution": "NodeNext",
    "newLine": "LF",
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "outDir": ".nowhere",
    "removeComments": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "inlineSources": true,
    "strict": true,
    "stripInternal": true,
    "target": "ES2019",
    "useDefineForClassFields": true,
  }
}

{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"module": "Node16",
		"moduleResolution": "Node16",
		"newLine": "LF",
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"outDir": ".nowhere",
		"removeComments": false,
		"resolveJsonModule": true,
		"sourceMap": true,
		"inlineSources": true,
		"strict": true,
		"stripInternal": true,
		"target": "ES2019",
		"useDefineForClassFields": true
	}
}

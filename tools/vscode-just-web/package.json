{
  "name": "vscode-just-web",
  "displayName": "vscode-just-web",
  "version": "0.0.1",
  "description": "",
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "build": "tsc -p ./",
    "build:watch": "tsc -p ./",
    "clean": "rimraf out",
    "depcheck": "echo no depcheck yet",
    "lint": "eslint --ext=ts,tsx,js,yaml,yml .",
    "pretest": "npm run build && npm run lint",
    "reset": "rimraf .vscode-test",
    "test": "node ./out/test/runTest.js",
    "test:watch": "echo no test watch",
    "verify": "npm-run-all clean -p build lint -s test",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "commands": [
      {
        "command": "vscode-just-web.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "activationEvents": [
    "onCommand:vscode-just-web.helloWorld"
  ],
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.26",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "~8.5.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "rimraf": "~3.0.2",
    "typescript": "^4.6.3",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "vscode": "^1.66.0"
  }
}

{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "allowMostlyStandardPackageNames": true,
  "ensureConsistentVersions": true,
  "eventHooks": {
    "postRushBuild": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "preRushInstall": []
  },
  "gitPolicy": {
    "allowedEmailRegExps": [
      "[^@]+@users\\.noreply\\.github\\.com"
    ],
    "sampleEmail": "example@users.noreply.github.com",
    "changeLogUpdateCommitMessage": "chore: update changelogs [skip ci]",
    "versionBumpCommitMessage": "chore: bump versions [skip ci]"
  },
  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0 || >=16.13.0 <17.0.0",
  "pnpmOptions": {
    "useWorkspaces": true
  },
  "pnpmVersion": "6.32.3",
  "projectFolderMaxDepth": 2,
  "projectFolderMinDepth": 2,
  "projects": [
    {
      "packageName": "just-func-play-solid",
      "projectFolder": "just-func/play-solid"
    },
    {
      "packageName": "just-func-play-react",
      "projectFolder": "just-func/play-react"
    }
  ],
  "repository": {
    "url": "https://github.com/justland/just-web",
    "defaultBranch": "main"
  },
  "rushVersion": "5.63.0",
  "variants": []
}

{
	"name": "@just-web/{{name}}",
	"version": "0.0.0",
	"description": "just-web {{name}} plugin module",
	"keywords": [
		"just-web",
		"framework"
	],
	"homepage": "https://github.com/justland/just-web/tree/main/plugins/{{name}}",
	"bugs": {
		"url": "https://github.com/justland/just-web/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/justland/just-web.git",
		"directory": "plugins/{{name}}"
	},
	"license": "MIT",
	"author": {
		"name": "Homa Wong (unional)",
		"email": "homawong@gmail.com",
		"url": "https://github.com/unional"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		"types": "./esm/index.d.ts",
		"import": "./esm/index.js",
		"require": "./cjs/index.js",
		"default": "./cjs/index.js"
	},
	"main": "./cjs/index.js",
	"module": "./esm/index.js",
	"types": "./esm/index.d.ts",
	"files": [
		"cjs",
		"esm",
		"ts",
		"!**/*.{spec,test,unit,accept,integrate,system,perf,stress}.*"
	],
	"scripts": {
		"build": "run-p build:cjs build:esm",
		"build:cjs": "tsc -p tsconfig.cjs.json && ncp ../../config/package.cjs.json cjs/package.json",
		"build:esm": "tsc",
		"build:watch": "tsc -w",
		"clean": "rimraf cjs esm coverage lib --glob *.tsbuildinfo",
		"coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest --coverage",
		"depcheck": "depcheck",
		"lint": "cross-env TIMING=1 eslint --ext=ts,tsx,js,yaml,yml .",
		"nuke": "rimraf node_modules",
		"size": "size-limit",
		"test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
		"test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest --watch",
		"verify": "npm-run-all -p build lint coverage depcheck -p size"
	},
	"devDependencies": {
		"@just-web/log": "workspace:^",
		"@just-web/repo-scripts": "workspace:^",
		"@just-web/testing": "workspace:^",
		"@repobuddy/jest": "^2.2.1",
		"@size-limit/preset-small-lib": "~8.1.0",
		"@types/jest": "^29.2.3",
		"@types/node": "^18.0.0",
		"cross-env": "^7.0.3",
		"depcheck": "~1.4.3",
		"eslint": "^8.11.0",
		"eslint-plugin-harmony": "^7.1.1",
		"jest": "^29.3.1",
		"jest-validate": "^29.3.1",
		"jest-watch-suspend": "^1.1.2",
		"jest-watch-toggle-config-2": "^2.1.0",
		"jest-watch-typeahead": "^2.2.1",
		"ncp": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "~3.0.2",
		"size-limit": "~8.1.0",
		"ts-jest": "^29.0.3",
		"typescript": "^4.9.3"
	},
	"peerDependencies": {
		"@just-web/log": "workspace:^",
		"@just-web/types": "workspace:^"
	},
	"typedoc": {
		"entryPoint": "./ts/index.ts",
		"readmeFile": "./README.md"
	}
}

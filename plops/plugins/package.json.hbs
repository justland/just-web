{
	"name": "@just-web/{{name}}",
	"version": "0.0.0",
	"description": "just-web {{name}} plugin module",
	"keywords": [
		"just-web",
		"framework"
	],
	"homepage": "https://github.com/justland/just-web/tree/main/plugins/{{name}}",
	"bugs": {
		"url": "https://github.com/justland/just-web/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/justland/just-web.git",
		"directory": "plugins/{{name}}"
	},
	"license": "MIT",
	"author": {
		"name": "Homa Wong (unional)",
		"email": "homawong@gmail.com",
		"url": "https://github.com/unional"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		"types": "./esm/index.d.ts",
		"import": "./esm/index.js",
		"require": "./cjs/index.cjs",
		"default": "./cjs/index.cjs"
	},
	"main": "./cjs/index.cjs",
	"module": "./esm/index.js",
	"types": "./esm/index.d.ts",
	"files": [
		"cjs",
		"esm",
		"ts",
		"!**/*.{spec,test,unit,accept,integrate,system,perf,stress}.*"
	],
	"scripts": {
		"build": "run-p build:cjs build:esm",
		"build:cjs": "tsc -p tsconfig.cjs.json && ncp ../../config/package.cjs.json cjs/package.json",
		"build:esm": "tsc",
		"build:watch": "tsc -w",
		"clean": "rimraf cjs esm coverage lib --glob *.tsbuildinfo",
		"coverage": "vitest run --coverage",
		"nuke": "rimraf node_modules",
		"size": "size-limit",
		"test": "vitest run",
		"test:watch": "vitest",
		"verify": "npm-run-all -p build lint coverage -p size"
	},
	"devDependencies": {
		"@just-web/log": "workspace:^",
		"@just-web/repo-scripts": "workspace:^",
		"@just-web/testing": "workspace:^",
		"@repobuddy/vitest": "catalog:",
		"@size-limit/preset-small-lib": "~8.1.0",
		"@types/node": "^18.0.0",
		"@vitest/coverage-v8": "^4.0.15",
		"cross-env": "^7.0.3",
		"ncp": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "~3.0.2",
		"size-limit": "~8.1.0",
		"typescript": "^4.9.3",
		"vitest": "^4.0.15"
	},
	"peerDependencies": {
		"@just-web/log": "workspace:^",
		"@just-web/types": "workspace:^"
	},
	"typedoc": {
		"entryPoint": "./ts/index.ts",
		"readmeFile": "./README.md"
	}
}

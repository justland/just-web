{
	"name": "examples-browser",
	"version": "0.0.0",
	"private": true,
	"description": "Browser examples for just-web",
	"keywords": [
		"just-web"
	],
	"homepage": "https://github.com/justland/just-web/tree/main/examples/browser#readme",
	"bugs": {
		"url": "https://github.com/justland/just-web/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/justland/just-web.git",
		"directory": "examples/browser"
	},
	"license": "MIT",
	"author": {
		"name": "Homa Wong (unional)",
		"email": "homawong@gmail.com",
		"url": "https://github.com/unional"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./esm/index.d.mts",
				"default": "./esm/index.mjs"
			},
			"require": {
				"types": "./cjs/index.d.cts",
				"default": "./cjs/index.cjs"
			},
			"default": "./esm/index.mjs"
		}
	},
	"main": "./cjs/index.cjs",
	"module": "./esm/index.mjs",
	"types": "./esm/index.d.mts",
	"files": [
		"cjs",
		"esm",
		"src",
		"!**/*.{spec,test,unit,accept,integrate,system,perf,stress}.*"
	],
	"scripts": {
		"build": "tsdown",
		"clean": "rimraf cjs esm coverage --glob *.tsbuildinfo",
		"coverage": "vitest run --coverage",
		"nuke": "rimraf node_modules",
		"size": "size-limit",
		"test": "vitest run",
		"test:watch": "vitest",
		"verify": "npm-run-all -p build lint coverage -p size"
	},
	"dependencies": {
		"@just-web/app": "workspace:^",
		"@just-web/commands": "workspace:^",
		"@just-web/presets-browser": "workspace:^",
		"@just-web/states": "workspace:^"
	},
	"devDependencies": {
		"@just-web/repo-scripts": "workspace:^",
		"@repobuddy/typescript": "^2.1.0",
		"@repobuddy/vitest": "catalog:",
		"@size-limit/preset-small-lib": "~12.0.0",
		"@vitest/browser-playwright": "^4.0.15",
		"@vitest/coverage-v8": "^4.0.15",
		"npm-run-all": "^4.1.5",
		"repobuddy": "^1.0.1",
		"rimraf": "~6.1.2",
		"size-limit": "~12.0.0",
		"tsdown": "^0.18.3",
		"vitest": "^4.0.15"
	}
}
